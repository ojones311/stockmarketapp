   if(Object.keys(stockStats).length === 0){\n            await fetchStockStats()\n        }\n        if(moreInfo){\n            setMoreInfo(false) \n        }else if(!moreInfo){\n            setMoreInfo(true)\n        }\n    }\n\n    const handleStockNews = async () => {\n        if(stockNews.length < 1){\n            await fetchCompanyNews()\n        }\n        if(toggleNews){\n            setToggleNews(false)\n        }else if(!toggleNews){\n            setToggleNews(true)\n        }\n    }\n\n    const truncateNumber = (num) => {\n       return Number.parseFloat(num).toFixed(4)\n    }\n\n    useEffect(()=> {\n        fetchStockInfo()\n    },[])\n\n    return (\n        <div className='stock-page'>\n           <div className='stockInfo'>\n                <h2>{stockData.companyName}</h2>\n                <h4>{stockData.symbol}</h4>\n                <a href={stockData.website}>{stockData.website}</a>\n                <p>{stockData.description}</p>\n           </div>\n           <div className= 'more-info-button'>\n                <button onClick={handleStockInformation}>Show Financials</button>\n           </div>\n        \n           {moreInfo ? <div className='stock-stats'>\n               <p><b>{'52 Week High: '}</b>{stockStats.week52high}</p>\n               <p><b>{'52 Week Low: '}</b>{stockStats.week52low}</p>\n               <p><b>{'YTD Change Percentage: '}</b>{truncateNumber(stockStats.ytdChangePercent)}</p>\n               <p><b>{'Price to Earnings Ratio: '}</b>{truncateNumber(stockStats.peRatio)}</p>\n               <p><b>{'Market Cap: '}</b>{stockStats.marketcap.toLocaleString()}</p>\n           </div> : <div></div>}\n           <div className='toggleNews-button'>\n                <button onClick={handleStockNews}>Show Related News</button>\n           </div>\n\n           {toggleNews ? stockNews.map((article) => {\n               return (\n                 <div className='stock-news'>\n                     <h5>{article.source}</h5>\n                     <a href={article.url}>{article.headline}</a>\n                     <p>{article.summary}</p>\n                </div>\n               )\n           }): <div></div>}\n        </div>\n    )\n}\n\n\nexport default StockPage","/Users/owenjones/Personal_Projects/stockmarketapp/frontend/src/Components/AboutPage.jsx",[],"/Users/owenjones/Personal_Projects/stockmarketapp/frontend/src/Components/LandingPage.jsx",[],"/Users/owenjones/Personal_Projects/stockmarketapp/frontend/src/Components/SearchStockForm.jsx",[],"/Users/owenjones/Personal_Projects/stockmarketapp/frontend/src/Components/useSubmitForm.jsx",[],"/Users/owenjones/Personal_Projects/stockmarketapp/frontend/src/secrets.js",[],"/Users/owenjones/Personal_Projects/stockmarketapp/frontend/src/apiURL.js",[],"/Users/owenjones/Personal_Projects/stockmarketapp/frontend/src/context/AuthContext.js",[],"/Users/owenjones/Personal_Projects/stockmarketapp/frontend/src/adapters/firebase.js",[],"/Users/owenjones/Personal_Projects/stockmarketapp/frontend/src/Components/Signin.jsx",["87"],"/Users/owenjones/Personal_Projects/stockmarketapp/frontend/src/Components/Signup.jsx",[],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":2,"message":"93","line":26,"column":64,"nodeType":"94","messageId":"95","endLine":26,"endColumn":77},{"ruleId":"96","severity":1,"message":"97","line":12,"column":12,"nodeType":"94","messageId":"98","endLine":12,"endColumn":23},{"ruleId":"96","severity":1,"message":"99","line":12,"column":25,"nodeType":"94","messageId":"98","endLine":12,"endColumn":39},{"ruleId":"100","severity":1,"message":"101","line":24,"column":51,"nodeType":"102","messageId":"103","endLine":24,"endColumn":53},{"ruleId":"104","severity":1,"message":"105","line":90,"column":7,"nodeType":"106","endLine":90,"endColumn":14,"suggestions":"107"},{"ruleId":"108","severity":1,"message":"109","line":31,"column":25,"nodeType":"110","endLine":31,"endColumn":52},{"ruleId":"104","severity":1,"message":"111","lin